{
  "kind": "collectionType",
  "collectionName": "listings",
  "info": {
    "singularName": "listing",
    "pluralName": "listings",
    "displayName": "Listing",
    "description": "Объявление недвижимости"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext"
    },
    "multicurrencyprice": {
      "type": "component",
      "component": "meta.multi-currency-price",
      "repeatable": false
    },
    "property_type": {
      "type": "enumeration",
      "enum": [
        "apartment",
        "house",
        "garage",
        "land",
        "commercial",
        "room",
        "event_venue"
      ]
    },
    "deal_type": {
      "type": "enumeration",
      "enum": [
        "sale",
        "long_term_rent",
        "short_term_rent"
      ]
    },
    "location": {
      "type": "component",
      "component": "shared.contact",
      "repeatable": false
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false
    },
    "building_info": {
      "type": "component",
      "component": "property.building-info",
      "repeatable": false
    },
    "apartment_info": {
      "type": "component",
      "component": "property.apartment-info",
      "repeatable": false
    },
    "land_info": {
      "type": "component",
      "component": "property.land-info",
      "repeatable": false
    },
    "garage_info": {
      "type": "component",
      "component": "property.garage-info",
      "repeatable": false
    },
    "commercial_info": {
      "type": "component",
      "component": "property.commercial-info",
      "repeatable": false
    },
    "features": {
      "type": "component",
      "component": "shared.feature",
      "repeatable": true
    },
    "price_history": {
      "type": "component",
      "component": "meta.price-change",
      "repeatable": true
    },
    "contact": {
      "type": "component",
      "component": "shared.contact",
      "repeatable": false
    }
  }
}
