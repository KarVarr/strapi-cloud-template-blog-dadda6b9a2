  "collectionName": "apartments",
  "info": {
    "singularName": "apartment",
    "pluralName": "apartments",
    "displayName": "Apartment"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "price": {
      "type": "decimal"
    },
    "address": {
      "type": "string"
    },
    "city": {
      "type": "string"
    },
    "rooms": {
      "type": "string"
    },
    "area": {
      "type": "decimal"
    },
    "statusApartment": {
      "type": "enumeration",
      "enum": [
        "available",
        "sold",
        "rented"
      ]
    },
    "images": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
  }
